<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.mhyran.spacequizz2">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:requestLegacyExternalStorage="true"
        android:icon="@mipmap/logosqroom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logosqroom_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpaceQuizz2">

        <activity android:name=".Controllers.HighscoreActivity" />
        <activity android:name=".Controllers.RegisterActivity" />
        <activity android:name=".Controllers.LoginActivity" />
        <activity android:name=".Controllers.GameActivity" />
        <activity android:name=".Controllers.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>